## Environment ##
NODE_ENV=development


## Server ##
PORT=3000
HOST=localhost


# Setup jet-logger ##
JET_LOGGER_MODE=CONSOLE
JET_LOGGER_FILEPATH=jet-logger.log
JET_LOGGER_TIMESTAMP=TRUE
JET_LOGGER_FORMAT=LINE

## Authentication ##
# SECURE_COOKIE 'false' here for demo-ing. But ideally should be true.
COOKIE_DOMAIN=localhost
COOKIE_PATH=/
SECURE_COOKIE=false
JWT_SECRET=xxxxxxxxxxxxxx
COOKIE_SECRET=xxxxxxxxxxxxxx
# expires in 3 days
COOKIE_EXP=259200000


MONGODB_URL=mongodb://localhost:27017
WHATSAPP_TIMEOUT=1;

NEXT_PUBLIC_GYM_BARCODE_TITLE="FITNESS GYM"
NEXT_PUBLIC_GYM_PHONE_COUNTRY="EG"
